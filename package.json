{
  "name": "kombi-cli",
  "version": "0.0.23-beta",
  "description": "Kombi CLI",
  "author": "HDN DIgital",
  "bin": {
    "kombi": "./bin/run"
  },
  "homepage": "https://github.com/hdntecnologiabr/hdn-kombi-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "hdntecnologiabr/hdn-kombi-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postinstall": "chmod +x ./bin/run && chmod +x ./bin/dev",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepare": "husky install",
    "kombi": "./bin/run",
    "kombi:dev": "./bin/dev"
  },
  "dependencies": {
    "@oclif/core": "^1.6.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-plugins": "^2.1.12",
    "chalk": "^5.0.1",
    "degit": "^2.8.4",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.1",
    "replace-in-file": "^6.3.2",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.0",
    "@types/chai": "^4.3.0",
    "@types/degit": "^2.8.3",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/shelljs": "^0.8.11",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "mocha": "^9",
    "oclif": "^2.6.0",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "4.4.3"
  },
  "oclif": {
    "bin": "kombi",
    "dirname": "kombi",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/hdntecnologiabr/hdn-kombi-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
